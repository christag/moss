{
  "name": "moss",
  "version": "0.1.0",
  "private": true,
  "description": "M.O.S.S. - Material Organization & Storage System: IT Asset Management Platform",
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint-staged": "lint-staged",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:migrate": "ts-node --compiler-options '{\"module\":\"commonjs\"}' src/lib/migrate.ts",
    "db:rebuild": "node scripts/db/rebuild-database.js",
    "db:check-indexes": "node scripts/db/check-missing-fk-indexes.js",
    "security:check-xss": "node scripts/security/check-legacy-xss.js",
    "prepare": "node -e \"if (process.env.HUSKY === '0' || !require('fs').existsSync('.git')) process.exit(0); require('child_process').execSync('husky', {stdio: 'inherit'})\""
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.908.0",
    "@aws-sdk/s3-request-presigner": "^3.908.0",
    "@modelcontextprotocol/sdk": "^1.20.0",
    "@types/cytoscape": "^3.21.9",
    "@types/papaparse": "^5.3.16",
    "@types/react-window": "^1.8.8",
    "bcryptjs": "^3.0.2",
    "cytoscape": "^3.33.1",
    "cytoscape-cola": "^2.5.1",
    "cytoscape-svg": "^0.4.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express-rate-limit": "^8.1.0",
    "framer-motion": "^12.23.24",
    "html2canvas": "^1.4.1",
    "jose": "^6.1.0",
    "jsbarcode": "^3.12.1",
    "jspdf": "^3.0.3",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "nanoid": "^5.1.6",
    "next": "^15.5.6",
    "next-auth": "^5.0.0-beta.29",
    "papaparse": "^5.5.3",
    "pg": "^8.13.1",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-countup": "^6.5.3",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-signature-canvas": "^1.1.0-alpha.2",
    "react-window": "^2.2.0",
    "sonner": "^2.0.7",
    "xlsx": "^0.18.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.37.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.14",
    "@types/jsbarcode": "^3.11.4",
    "@types/node": "^22.0.0",
    "@types/pg": "^8.11.10",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.0.0",
    "@types/react-big-calendar": "^1.16.3",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix --max-warnings=20",
      "prettier --write"
    ],
    "src/**/*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
